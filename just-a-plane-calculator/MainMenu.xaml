<Window x:Class="just_a_plane_calculator.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:just_a_plane_calculator"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid>
        <UniformGrid Grid.Row="1" Rows="5" Margin="5,5,5,5" Width="300" HorizontalAlignment="Left">

            <Button Name="CalcApplet" Click="LaunchCalculator" Content="Calculator"></Button>

        </UniformGrid>
    </Grid>
</Window>
